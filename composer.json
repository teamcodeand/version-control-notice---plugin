{
  "name": "teamcodeand/version-control-notice",
  "type": "wordpress-muplugin",
  "license": "MIT",
  "description": "Shows a client friendly explainer as to why installing or updating plugins is disabled",
  "authors": [
    {
      "name": "Dale Grant",
      "email": "dale@codeand.com.au",
      "homepage": "https://github.com/dalepgrant"
    }
  ],
  "support": {
    "issues": "https://github.com/teamcodeand/version-control-notice/issues"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:teamcodeand/codeand-wp-coding-standards.git",
      "no-api": true
    }
  ],
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.12.1",
    "roave/security-advisories": "dev-latest",
    "teamcodeand/codeand-wp-coding-standards": "dev-main"
  },
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
